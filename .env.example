# Convex Configuration (recommended backend)
# 1) Run: npm run convex:dev
# 2) Copy deployment URL and site URL from Convex dashboard/CLI
VITE_CONVEX_URL=your_convex_deployment_url
VITE_CONVEX_SITE_URL=your_convex_site_url

# Gemini AI Configuration
# Get from: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key

# Optional override for non-Convex backend gateway
VITE_ROADRUNNER_API_BASE=
